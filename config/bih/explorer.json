        },
        {
          "field": "PatientSex",
          "name": "Gender"
        },
        {
          "field": "primary_site",
          "name": "Primary Site"
        },
        {
          "field": "disease_type",
          "name": "Disease Type"
        },
        {
          "field": "commons_name",
          "name": "Platform"
        },
        {
          "field": "collection_id",
          "name": "Collection ID"
        },
        {
          "field": "license",
          "name": "License"
        },
        {
          "field": "data_url_doi",
          "name": "DOI"
        }
      ],
      "manifestMapping": {
        "resourceIndexType": "imaging_series",
        "resourceIdField": "StudyInstanceUID",
