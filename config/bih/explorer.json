[
  {
    "tabTitle": "Series",
    "charts": {
      "commons_name": {
        "chartType": "fullPie",
        "title": "Platform"
      },
      "primary_site": {
        "chartType": "fullPie",
        "title": "Primary Site"
      },
      "disease_type": {
        "chartType": "fullPie",
        "title": "Disease Type"
      }
