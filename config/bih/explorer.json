      "fields": [
        "dicom_viewer_url",
        "StudyInstanceUID",
        "StudyDescription",
        "BodyPartExamined",
        "primary_site",
        "disease_type",
        "commons_name",
        "collection_id",
        "license",
        "data_url_doi",
        "SeriesInstanceUID",
        "SeriesDescription"
